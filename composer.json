{
    "name": "laratree/laravel-link",
    "description": "Associate models with URL links",
    "version": "0.0.1",
    "keywords": [
        "laratree",
        "laravel",
        "laravel-link"
    ],
    "homepage": "https://github.com/laratree/laravel-link",
    "license": "MIT",
    "authors": [
        {
            "name": "MimisK",
            "email": "mimisk88@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {

    },
    "require-dev": {
        "orchestra/testbench": "^8.13"
    },
    "autoload": {
        "psr-4": {
            "Laratree\\LaravelLink\\": "src/",
            "Laratree\\LaravelLink\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laratree\\LaravelLink\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-link --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laratree\\LaravelLink\\LaravelLinkServiceProvider"
            ],
            "aliases": {
                "Laravel-Link": "Laratree\\LaravelLink\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
